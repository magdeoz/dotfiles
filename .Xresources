#include "/home/magdeoz/.termcolors/shekar"

Xft.dpi:				101
Xft.antialias:			1
Xft.autohint:			0
Xft.rgba:				rgb
Xft.hinting:			1
Xft.hintstyle:			hintfull
Xft.lcdfilter:			lcdlight

*dpi:				101
*antialias:		1
*autohint:			0
*rgba:				rgb
*hinting:			1
*hintstyle:		hintfull
*lcdfilter:		lcddefault

Emacs.menuBar: off
Emacs.toolBar: off
Emacs.verticalScrollBars: off
Emacs.horizontalScrollBars: off
Emacs.font: Fantasque Sans Mono-13:autohint=yes

*.vt100.faceName:Hack:style=Regular:pixelsize=12:antialias=true:autohint=true;

*.vt100.font: 7x11
XTerm.vt100.renderFont: true
XTerm*boldFont: 				xft:Inconsolata for Powerline:style=Bold:pixelsize=14:antialias=true:hinting=true
XTerm*lineSpace:                0
!*letterSpace: 				-9
XTerm.iso14755:                 false
XTerm.iso14755_52:				false
XTerm.scaleHeight: 1.01
xterm*scaleHeight: 1.00
XTerm*borderWidth: 17
*internalBorder: 5
URxvt.scaleHeight: 1.01

XTerm.termName: 				xterm-256color
URxvt*termName:                 rxvt-256color
URxvt*font:                     xft:Hack:style=Regular:pixelsize=12:antialias=true:hinting=1
URxvt.depth:                    32
URxvt*lineSpace:                -1
!*.letterSpace: -9

URxvt.iso14755:                 false
URxvt.iso14755_52:              false
*.geometry:                 104x23
URxvt.scrollBar:                false
URxvt*loginShell:               true
URxvt.perl-ext-common:          default,clipboard,selection-to-clipboard
URxvt.keysym.Shift-Control-c:   perl:clipboard:copy
URxvt.keysym.Shift-Control-v:   perl:clipboard:paste
URxvt.clipboard.autocopy:       true

xterm*VT100.Translations: #override \
                 Ctrl Shift <Key>V:    insert-selection(CLIPBOARD) \n\
                 Ctrl Shift <Key>C:    copy-selection(CLIPBOARD)


*.solid-background: #1d1f21
*.cursorColor:  bblk
*background: bg
*foreground: fg
*borderColor: bg
*color0:  blk
*color8:  bblk
*color1:  red
*color9:  bred
*color2:  grn
*color10:  bgrn
*color3:  ylw
*color11:  bylw
*color4:  blu
*color12:  bblu
*color5:  mag
*color13:  bmag
*color6:  cyn
*color14:  bcyn
*color7:  wht
*color15:  bwht

rofi.color-enabled: true
rofi.color-window: bg, bg, bg
rofi.color-normal: bg, fg, bg, red
rofi.color-active: red
rofi.color-urgent: red

!rofi.lines:                        2
rofi.separator-style:               solid
rofi.width:                         33
rofi.height:                        33
rofi.line-padding:                  7
rofi.hide-scrollbar:                True
rofi.columns:                       1

! xst

! ref: https://github.com/gnotclub/xst/blob/master/doc/Xresources
st.termname:     st-256color
st.shell:        /bin/zsh
st.font:         Iosevka Term:style=Regular:pixelsize=12:antialias=true:autohint=true;
! Available cursor values: 2 4 6 7 = █ _ | ☃ ( 1 3 5 are blinking versions)
st.cursorshape:   2
st.bold_font:     1
st.chscale:       1.0
st.cwscale:       1.0
